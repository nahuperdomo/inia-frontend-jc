{
    "extends": [],
    "rules": {
        "at-rule-no-unknown": [
            true,
            {
                "ignoreAtRules": [
                    "tailwind",
                    "apply",
                    "variants",
                    "responsive",
                    "screen",
                    "layer",
                    "import",
                    "custom-variant",
                    "theme",
                    "keyframes"
                ]
            }
        ],
        "declaration-block-trailing-semicolon": null,
        "no-descending-specificity": null,
        "import-notation": null
    }
}